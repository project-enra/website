{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/Images/*", "/*.{css,scss,sass,js,json,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".css": "text/css",
    ".js": "text/javascript",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".gif": "image/gif",
    ".svg": "image/svg+xml",
    ".ico": "image/x-icon"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000"
  },
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    }
  ]
}
