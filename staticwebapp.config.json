{
  "routes": [
    {
      "route": "/index.html",
      "redirect": "/",
      "statusCode": 301
    },
    {
      "route": "/veilglass.html",
      "redirect": "/veilglass",
      "statusCode": 301
    },
    {
      "route": "/codex/index.html",
      "redirect": "/codex/",
      "statusCode": 301
    },
    {
      "route": "/codex/realms/hook/index.html",
      "redirect": "/codex/realms/hook/",
      "statusCode": 301
    },
    {
      "route": "/*.html",
      "redirect": "/*",
      "statusCode": 301
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
